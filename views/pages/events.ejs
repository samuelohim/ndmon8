<div class="jumbotron text-center">
	<h1>All Events</h1>
</div>

<table class="table table-bordered table-hover table-striped">
	
	<thead>
		<tr>
			<th>Event Name</th>
			<th>Description</th>
			<th></th>
		</tr>
	</thead>

	<tbody>
		<% for (var event of shijian) {%>
			<tr>
				<td><%=event.Pay %></td>
				<td><%=event.mid %></td>
				<td><a href="/events/<%=event.mid %>" class = 'btn btn-sm btn-primary'>View</a></td>
			</tr>
		<%}%>
	</tbody>
</table>